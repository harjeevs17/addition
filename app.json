{
  "expo": {
    "name": "Popcorn",
    "slug": "Popcorn",
    "platforms": ["ios", "android", "web"],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/popcorn.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
